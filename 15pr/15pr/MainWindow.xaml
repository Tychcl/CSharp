<Window x:Class="_15pr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_15pr"
        mc:Ignorable="d"
        Title="TimeLord" Width="320" MinWidth="320" Background="DarkCyan">
    <Grid>
        <StackPanel Width="300">
            <Frame x:Name="frame" NavigationUIVisibility="Hidden" Width="300"/>
            <Grid>
                <Button Content="Секундомер" Click="GoSec" HorizontalAlignment="Left" Margin="0,15,0,0" Width="150"/>
                <Button Content="Таймер" Click="GoTimer" Margin="0,15,0,0" HorizontalAlignment="Right" Width="150"/>
            </Grid>
        </StackPanel>
    </Grid>
</Window>
