<Page x:Class="pr33.Pages.SignIn"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pr33.Pages"
      mc:Ignorable="d"
      Title="SignIn" Margin="15" Background="Gray">
    <Grid Margin=" 0 0 0 15">
        <StackPanel>
            <Label Content="Телефон или почта" FontSize="14" Margin="10 5 10 0"/>
            <TextBox x:Name="phonemail" Margin="10 0 10 0"/>
            <Label Content="Пароль" FontSize="14" Margin="10 5 10 0"/>
            <TextBox x:Name="password" Margin="10 0 10 10"/>
            <Grid>
                <Button Margin="10 0 0 0" Click="back" HorizontalAlignment="Left" Content="Регистрация"/>
                <Button Margin="0 0 10 0" Click="sign" HorizontalAlignment="Right" Content="Войти"/>
            </Grid>
        </StackPanel>
    </Grid>
</Page>
