<Page x:Class="pr33.Pages.Messanger"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pr33.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Messanger">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel x:Name="users"  Grid.Column="0"/>
        <Button Click="logout" Content="Выйти из аккаунта" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
        <StackPanel Grid.Column="1" VerticalAlignment="Bottom">
            <StackPanel x:Name="message"/>
            <TextBox KeyDown="enter" x:Name="text"/>
        </StackPanel>
    </Grid>
</Page>
