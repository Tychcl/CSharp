<Window x:Class="pr27.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pr27"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="350" Height="350" MinWidth="350" Width="350" Background="Gray">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="40" VerticalAlignment="Top">
            <Border BorderBrush="#FF000000" BorderThickness="2" CornerRadius="3" Background="White" Margin="5" Width="75">
                <Button x:Name="thet" Content="Клубы" BorderThickness="0" Margin="2" Background="White" Click="got"/>
            </Border>
            <Border BorderBrush="#FF000000" BorderThickness="2" CornerRadius="3" Background="White" Margin="5" Width="75">
                <Button x:Name="afi" Content="Аренда" BorderThickness="0" Margin="2" Background="White" Click="goa"/>
            </Border>
        </StackPanel>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Margin="0,30,0,0"/>
    </Grid>
</Window>
